<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.collections.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.collections.FXCollections ?>

<AnchorPane prefHeight="417.0" prefWidth="482.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ReportController">
   <children>
      <Pane layoutX="-124.0" layoutY="-148.0" prefHeight="200.0" prefWidth="200.0" />
      <TextArea fx:id="txtReport" layoutX="19.0" layoutY="118.0" prefHeight="280.0" prefWidth="426.0" />
      <ComboBox fx:id="cmbReport" layoutX="40.0" layoutY="40.0" prefWidth="150.0">
         <items>
            <FXCollections fx:factory="observableArrayList">
                <String fx:value="Accounts" />
                <String fx:value="Books" />
                <String fx:value="Past Due" />
                <String fx:value="Checked Out" />
            </FXCollections>
        </items>
      	</ComboBox>
      <Button fx:id="btnReport" layoutX="223.0" layoutY="40.0" mnemonicParsing="false" onAction="#runReport" text="Run Report" />
      <Button fx:id="btnBack" layoutX="383.0" layoutY="40.0" mnemonicParsing="false" onAction="#sceneBack" text="Back" />
   </children>
</AnchorPane>
